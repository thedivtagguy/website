{
  "name": "svelte-starter",
  "version": "3.0.0",
  "scripts": {
    "gdoc": "npm run docs && node tasks/fetch-google.cjs",
    "docs": "node tasks/collect-docs.cjs",
    "thumbnail-stories": "node tasks/thumbnail-stories.cjs",
    "thumbnail-projects": "node tasks/thumbnail-projects.cjs",
    "thumbnail-other": "node tasks/thumbnail-other.cjs",
    "thumbnail": "npm run thumbnail-stories && npm run thumbnail-projects && npm run thumbnail-other && node tasks/thumbnail-blogposts.cjs",
    "thumbnail-resources": "node tasks/thumbnail-resources.cjs",
    "thumbnail-colour": "node tasks/thumbnail-colour.cjs",
    "dev:only": "svelte-kit dev",
    "build:only": "svelte-kit build",
    "preview": "svelte-kit preview",
    "tailwind:watch": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development  postcss src/styles/tailwind.css -o static/app.css -w",
    "tailwind:build": "cross-env TAILWIND_MODE=build cross-env NODE_ENV=production  postcss src/styles/tailwind.css -o static/app.css",
    "dev": "concurrently \"npm run dev:only\" \"npm run tailwind:watch\" \"npm run netlify\"",
    "build": "npm run gdoc && npm run netlify && npm run tailwind:build && npm run build:only cross-env NODE_ENV=production",
    "style": "node tasks/style-dictionary.cjs",
    "netlify": "node tasks/netlify.mjs",
    "gallery": "node tasks/gallery.cjs",
    "start": "svelte-kit start",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "logs": "node tasks/log/getLogs.cjs"
  },
  "devDependencies": {
    "@rollup/plugin-dsv": "2.0.2",
    "@sveltejs/adapter-static": "1.0.0-next.21",
    "@sveltejs/kit": "1.0.0-next.190",
    "archieml": "^0.5.0",
    "autoprefixer": "^10.4.0",
    "d3": "7.1.1",
    "feather-icons": "4.28.0",
    "google-drive-getfilelist": "^1.0.5",
    "jimp": "^0.16.1",
    "lodash.debounce": "4.0.8",
    "mdsvex": "^0.10.6",
    "netlify": "^10.1.1",
    "node-fetch": "^2.6.7",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "rollup-plugin-svg": "2.0.0",
    "style-dictionary": "3.0.3",
    "svelte": "3.44.0",
    "svelte-bricks": "^0.1.3",
    "svelte-fuzzy": "^2.1.0",
    "svelte-preprocess": "4.9.8",
    "svelte-twitter-widgets": "^0.1.0",
    "svelte-video-player": "^1.2.5",
    "tailwindcss": "^3.0.7",
    "vite-plugin-svgstring": "1.0.0"
  },
  "type": "module",
  "engines": {
    "node": ">= 14.17.3"
  },
  "browserslist": "> 0.5%, last 4 versions, not ie <= 11, not ie_mob <= 11",
  "thedivtagguy": {
    "subdirectory": ""
  },
  "dependencies": {
    "@untemps/svelte-readotron": "^1.5.2",
    "color": "^4.1.0",
    "colorthief": "^2.3.2",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "d3-timelines": "^1.3.1",
    "dotenv": "^10.0.0",
    "googleapis": "^92.0.0",
    "mkdirp": "^1.0.4",
    "node-vibrant": "^3.2.1-alpha.1",
    "postcss-cli": "^9.0.1",
    "postcss-nesting": "^8.0.1",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "remark-abbr": "^1.4.1",
    "remark-github": "^11.2.4",
    "typeface-playfair-display-sc": "^1.1.13",
    "typeface-source-sans-pro": "^1.1.13"
  }
}
